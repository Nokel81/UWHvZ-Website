{
  "name": "hvz-web",
  "version": "1.0.0",
  "author": "Sebastian Malton",
  "description": "This is the webserver & front end for the UW HvZ website and app",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "xo"
  },
  "xo": {
    "space": 4,
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "no-unused-vars": [
        "error",
        {
          "args": "none"
        }
      ],
      "unicorn/filename-case": "off",
      "no-use-extend-native/no-use-extend-native": "off",
      "import/no-unassigned-import": "off",
      "capitalized-comments": "off",
      "no-throw-literal": "off",
      "max-params": "off",
      "max-nested-callbacks": "off"
    },
    "globals": [
      "shouldCreateSourcemap",
      "fs",
      "path",
      "rootRequire",
      "window",
      "document",
      "google",
      "it",
      "describe",
      "beforeEach",
      "spyOn",
      "angular",
      "expect"
    ]
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.3",
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "angular-cookies": "^1.6.4",
    "angular-file-upload": "^2.5.0",
    "angular-ui-bootstrap": "^2.5.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.17.2",
    "brfs": "^1.4.3",
    "browserify": "^14.4.0",
    "browserify-ngannotate": "^2.0.0",
    "bulk-require": "^1.0.0",
    "bulkify": "^1.4.2",
    "crypto-random-string": "^1.0.0",
    "debowerify": "^1.5.0",
    "express": "^4.15.3",
    "gmail-send": "^1.2.10",
    "html_resolve": "^1.1.0",
    "mongoose": "^4.10.5",
    "mongoose-unique-validator": "^1.0.5",
    "morgan": "^1.8.2",
    "multiparty": "^4.1.3",
    "nodemailer": "^4.0.1",
    "object-path": "^0.11.4",
    "pbkdf2": "^3.0.12",
    "uglifyify": "^3.0.4"
  },
  "devDependencies": {
    "xo": "^0.18.2"
  }
}
